{
  "name": "rdf-preview",
  "displayName": "RDF Graph Preview",
  "description": "Preview RDF Graph inside VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "publisher": "LucienReboul",
  "categories": [
    "Visualization"
  ],
  "repository": "https://github.com/LucienRbl/rdf-preview.git",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rdf-preview.showGraph",
        "title": "Preview RDF Graph",
        "icon": "$(type-hierarchy)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "rdf-preview.showGraph",
          "when": "editorLangId == turtle || editorLangId == n-triples",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "n3": "^1.22.3"
  }
}
